<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

	<!-- Servlets -->

	<servlet>
		<servlet-name>altaTecnicoImpl</servlet-name>
		<servlet-class>isw2.presentacion.server.AltaTecnicoImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>altaTecnicoImpl</servlet-name>
		<url-pattern>/acme/altatecnico</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Acme.html</welcome-file>
	</welcome-file-list>

</web-app>
